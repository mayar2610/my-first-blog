      <scope doc="The Node interface is the primary datatype for the entire&#10;Document Object Model. It represents a single node in the&#10;document tree." ilk="class" name="Node">
        <variable attributes="static" citdl="Number" doc="The node is an Element." name="ELEMENT_NODE" />
