        <variable attributes="static" citdl="Number" doc="The value is an angle (deg). The value can be obtained by&#10;using the getFloatValue method." name="CSS_DEG" />
        <variable attributes="static" citdl="Number" doc="The value is an angle (rad). The value can be obtained by&#10;using the getFloatValue method." name="CSS_RAD" />
